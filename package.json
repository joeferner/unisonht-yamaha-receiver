{
  "name": "@unisonht/yamaha-receiver",
  "version": "0.0.1",
  "description": "UnisonHT Plugin for Yamaha Receivers",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "test:watch": "nodemon ./dist/test.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeferner/unisonht-yamaha-receiver.git"
  },
  "keywords": [
    "unisonht",
    "yamaha"
  ],
  "author": "Joe Ferner <joe@fernsroth.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeferner/unisonht-yamaha-receiver/issues"
  },
  "homepage": "https://github.com/joeferner/unisonht-yamaha-receiver#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27",
    "@unisonht/unisonht": "0.0.x"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^12.7.1",
    "@types/xmldom": "^0.1.29",
    "husky": "^3.0.3",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  }
}
