{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": false,
    "ordered-imports": false,
    "member-ordering": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "no-namespace": false
  }
}
